{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "journeyo/hero-section",
  "version": "0.1.0",
  "title": "Hero Section",
  "category": "widgets",
  "description": "A prominent hero section featuring a title, text, appStore links, and images.",
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "text": {
      "type": "string",
      "default": ""
    },
    "appStoreLink": {
      "type": "string",
      "default": ""
    },
    "googlePlayLink": {
      "type": "string",
      "default": ""
    },
    "buttonText": {
      "type": "string",
      "default": ""
    },
    "image": {
      "type": "object",
      "default": {
        "id": null,
        "url": "",
        "alt": ""
      }
    },
    "logo": {
      "type": "object",
      "default": {
        "id": null,
        "url": "",
        "alt": ""
      }
    },
    "logo2": {
      "type": "object",
      "default": {
        "id": null,
        "url": "",
        "alt": ""
      }
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "guten",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view-script.js"
}