{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "journeyo/benefits-section",
  "version": "0.1.0",
  "title": "Benefits Section",
  "category": "widgets",
  "description": "A customizable section to showcase key benefits with title, subtitle, and icons.",
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "subtitle": {
      "type": "string",
      "default": ""
    },
    "items": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "default": ""
          },
          "subtitle": {
            "type": "string",
            "default": ""
          },
          "icon": {
            "type": "object",
            "default": {
              "id": null,
              "url": "",
              "w": 0,
              "h": 0,
              "alt": ""
            },
            "properties": {
              "id": {
                "type": [
                  "integer",
                  "null"
                ]
              },
              "url": {
                "type": "string"
              },
              "w": {
                "type": "integer"
              },
              "h": {
                "type": "integer"
              },
              "alt": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "guten",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}